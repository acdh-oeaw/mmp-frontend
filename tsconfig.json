{
  "extends": "@stefanprobst/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "exactOptionalPropertyTypes": false,
    "paths": {
      "@/*": ["./src/*"],
      "~/*": ["./*"]
    },
    "types": ["d3", "leaflet", "node", "vuetify"]
  },
  "include": ["./env.d.ts", "./**/*.js", "./**/*.ts", "./**/*.vue"],
  "vueCompilerOptions": {
    "target": 2.7
  }
}
